---
import BaseLayout from '../layouts/BaseLayout.astro';
import CommandButton from '../components/CommandButton.astro';
import ResumeTimeline from '../components/ResumeTimeline.astro';

const experienceEntries = [
  {
    dateRange: '2022 – Present',
    company: 'Independent',
    title: 'Revenue Systems Architect',
    bullets: [
      'Built LLM-based lead qualification pipeline (Python, Claude/GPT) with tiered routing — auto, assisted, and human-required — integrated via Workato. Cut SDR qualification time 60%; lifted MQL-to-SQL conversion 34%.',
      'Designed ELT pipeline (Fivetran ingestion → Snowflake → dbt transformation layer) with stage-exit validation models and coverage-ratio tracking. Forecast accuracy improved from 67% to 84%.',
      'Rebuilt CRM data model across Salesforce and HubSpot — custom objects, validation rules, deduplication workflows, and integration mapping. Consolidated 11 revenue tools to 4; cut operational overhead 40%.',
      'Automated enrichment-to-routing pipeline (Workato orchestration, Census reverse ETL, LLM-based scoring) that produced $8M+ qualified pipeline and $350K/mo sustained throughput.',
    ],
  },
  {
    dateRange: '2021 – 2022',
    company: 'Benchling',
    title: 'Global Lead, Enterprise Development',
    bullets: [
      'Built enterprise SDR function from scratch — developed qualification frameworks for $100K+ ACV deals targeting life sciences accounts.',
      'Created outbound playbooks that became the standard for enterprise prospecting across the team.',
      'Partnered with marketing ops on lead scoring, routing, and attribution to improve handoff quality.',
    ],
  },
  {
    dateRange: '2020 – 2021',
    company: 'CloudZero',
    title: 'Cloud Cost Solutions Enablement',
    bullets: [
      'Enabled sales team on cloud cost optimization positioning — built demo environments and ROI models that shortened enterprise deal cycles.',
      'Developed competitive differentiation materials used across the sales org.',
    ],
  },
  {
    dateRange: '2019 – 2020',
    company: 'DataRobot',
    title: 'AI Solutions Specialist',
    bullets: [
      'Technical pre-sales for enterprise AI/ML platform — scoped and delivered proof-of-concept projects demonstrating predictive analytics ROI across financial services and insurance.',
    ],
  },
  {
    dateRange: '2018 – 2019',
    company: 'Smartsheet',
    title: 'Sales Development',
    bullets: [
      'Developed prospecting sequences adopted team-wide, consistently exceeded quota driving outbound pipeline for mid-market accounts.',
    ],
  },
  {
    dateRange: 'Earlier',
    company: 'UMass Amherst',
    title: 'Software Support & Network Engineering',
    bullets: [
      'Enterprise software support and campus network infrastructure.',
    ],
  },
];

const keyProjects = [
  {
    title: 'AI-Assisted Lead Routing + SLA Enforcement',
    metrics: 'Python + Claude/GPT, Workato orchestration · 45m first response, +25% MQL-to-SQL',
    slug: 'ai-routing-sla',
  },
  {
    title: 'Forecast Integrity + Operating Cadence',
    metrics: 'Snowflake + dbt semantic layer, BI dashboards · 84% forecast accuracy, <24h latency',
    slug: 'forecast-integrity-cadence',
  },
  {
    title: 'CRM Data Quality + Automated Governance',
    metrics: 'Salesforce + HubSpot data model, validation + dedup workflows · 92% field completion, -78% dupes',
    slug: 'data-governance-automation',
  },
  {
    title: 'Self-Serve Revenue Analytics',
    metrics: 'dbt semantic layer, self-serve BI · 80% reduction in ad-hoc requests',
    slug: 'self-serve-analytics',
  },
];
---

<BaseLayout title="Resume" description="Julian Ross — GTM engineer: ELT pipelines (Snowflake, dbt), workflow automation (Workato, Python), and LLM-based RevOps.">
  <article class="py-8 resume-page" data-section="about">
    <!-- Header -->
    <div class="mb-10">
      <h1 class="text-2xl md:text-3xl text-[var(--color-text)] mb-2">
        Julian Ross
      </h1>
      <p class="text-lg text-[var(--color-text-muted)] mb-4">
        Revenue Operations & GTM Systems
      </p>

      <!-- Contact row -->
      <div class="flex flex-wrap items-center gap-x-4 gap-y-2 text-sm text-[var(--color-text-muted)] mb-4">
        <a href="mailto:jross21@gmail.com" class="hover:text-[var(--color-accent)]">jross21@gmail.com</a>
        <span class="text-[var(--color-text-dim)]">|</span>
        <a href="https://www.linkedin.com/in/julianross21/" target="_blank" rel="noopener noreferrer" class="hover:text-[var(--color-accent)]">LinkedIn</a>
        <span class="text-[var(--color-text-dim)]">|</span>
        <a href="https://github.com/jross21" target="_blank" rel="noopener noreferrer" class="hover:text-[var(--color-accent)]">GitHub</a>
      </div>

      <CommandButton
        href="/julian-ross-resume.pdf"
        command="Download PDF"
        primary={true}
        section="about"
        class="print-hidden"
      />
    </div>

    <!-- Summary -->
    <section class="mb-10">
      <h2 class="resume-section-heading">Summary</h2>
      <p class="text-[var(--color-text-muted)] leading-relaxed max-w-2xl">
        Revenue Operations engineer with 10+ years building ELT pipelines (Snowflake, dbt, Fivetran), GTM automation (Workato, Python, Census), and LLM-based scoring and routing workflows. I work the full loop: data modeling, CRM architecture, workflow automation, and the reporting layer that proves it works. Strongest when I own the technical stack end-to-end and ship systems that revenue teams actually adopt.
      </p>
    </section>

    <!-- Experience -->
    <section class="mb-10">
      <h2 class="resume-section-heading">Experience</h2>
      <ResumeTimeline entries={experienceEntries} />
    </section>

    <!-- Key Projects -->
    <section class="mb-10">
      <h2 class="resume-section-heading">Key Projects</h2>
      <div class="space-y-4">
        {keyProjects.map((project) => (
          <div class="pl-4 border-l-2 border-[var(--color-border)]">
            <a href={`/projects/${project.slug}`} class="text-sm font-semibold text-[var(--color-text)] hover:text-[var(--color-accent)] print-link">
              {project.title}
            </a>
            <p class="text-xs text-[var(--color-text-muted)] mt-0.5">{project.metrics}</p>
          </div>
        ))}
      </div>
    </section>

    <!-- Skills & Tools -->
    <section class="mb-10">
      <h2 class="resume-section-heading">Skills & Tools</h2>
      <div class="grid grid-cols-1 sm:grid-cols-3 gap-4">
        <div>
          <p class="text-[var(--color-text)] font-semibold text-sm mb-2">CRM & Sales Ops</p>
          <div class="flex flex-wrap gap-2">
            {['Salesforce', 'HubSpot', 'Outreach', 'Salesloft', 'Gong', 'Clari'].map(tool => (
              <span class="tool-chip">{tool}</span>
            ))}
          </div>
        </div>
        <div>
          <p class="text-[var(--color-text)] font-semibold text-sm mb-2">Data & Analytics</p>
          <div class="flex flex-wrap gap-2">
            {['Snowflake', 'dbt', 'Looker', 'Tableau', 'Power BI', 'Fivetran'].map(tool => (
              <span class="tool-chip">{tool}</span>
            ))}
          </div>
        </div>
        <div>
          <p class="text-[var(--color-text)] font-semibold text-sm mb-2">Automation & AI</p>
          <div class="flex flex-wrap gap-2">
            {['Python', 'SQL', 'Zapier', 'Workato', 'Census', 'Claude / GPT'].map(tool => (
              <span class="tool-chip">{tool}</span>
            ))}
          </div>
        </div>
      </div>
    </section>

    <!-- Education -->
    <section>
      <h2 class="resume-section-heading">Education</h2>
      <p class="text-[var(--color-text)]">University of Massachusetts Amherst</p>
      <p class="text-sm text-[var(--color-text-muted)]">Information Technology</p>
    </section>
  </article>
</BaseLayout>

<style>
  .resume-section-heading {
    font-size: 0.75rem;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.08em;
    color: var(--color-section-about);
    margin-bottom: 1rem;
    padding-bottom: 0.5rem;
    border-bottom: 1px solid var(--color-border);
  }

  /* Print styles */
  @media print {
    .resume-page {
      padding: 0;
    }

    .resume-section-heading {
      color: #52525b;
      border-bottom-color: #e4e4e7;
    }

    .print-hidden {
      display: none !important;
    }

    .print-link::after {
      content: ' (' attr(href) ')';
      font-size: 0.625rem;
      color: #52525b;
      font-weight: 400;
    }
  }
</style>
